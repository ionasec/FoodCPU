<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

  <!-- Servlets -->
  
 
  <servlet>
  	<servlet-name>ServiceLogin</servlet-name>
  	<servlet-class>foodcpu.server.ServiceLoginImpl</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ServiceLogin</servlet-name>
  	<url-pattern>/appengine_drinkcounter/ServiceLogin</url-pattern>
  </servlet-mapping>

   <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>AppEngine_DrinkCounter.html</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>ServiceDrink</servlet-name>
  	<servlet-class>foodcpu.server.ServiceDrinkImpl</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ServiceDrink</servlet-name>
  	<url-pattern>/appengine_drinkcounter/ServiceDrink</url-pattern>
  </servlet-mapping>
  
</web-app>
